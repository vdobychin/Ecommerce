<script>
    $(function () {
        $('#modal-container').on('show.bs.modal', function (event) {
            var button = $(event.relatedTarget); // Button that triggered the modal
            var url = button.attr("href");
            var modal = $(this);
            modal.find('.modal-content').load(url);
        });
    });
</script>

