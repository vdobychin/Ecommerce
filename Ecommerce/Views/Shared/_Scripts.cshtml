<script>
    function phonenumber(obj) {
        var numbers = obj.value.replace(/\D/g, ''),
            char = { 0: '(', 3: ') ', 6: '-', 8: '-'};
        obj.value = '';
        for (var i = 0; i < numbers.length; i++) {
            obj.value += (char[i] || '') + numbers[i];
        }
    }
</script>

